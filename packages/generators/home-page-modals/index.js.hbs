/*
 *
 * `modalsConfig`: `@exsys-patient-insurance/home-page`.
 *
 */
import createLazyComponent from '@exsys-patient-insurance/react-lazy';
import { KeysOfRecord } from "@exsys-patient-insurance/types";

const MODALS_CONFIG = {
  {{#if hasNormalPackage}}
  {{#each normalPackage}}
  {{{ routePath }}}: createLazyComponent(() => import("@exsys-patient-insurance/{{{ packageName }}}" /* webpackChunkName: "exsys-patient-insurance.{{{packageName}}}" */)),
  {{/each}}
  {{/if}}
  {{#if hasLazyPackage}}
  {{#each lazyPackage}}
  {{{ routePath }}}: createLazyComponent(() => import("@exsys-patient-insurance/{{{ packageName }}}/src/component" /* webpackChunkName: "exsys-patient-insurance.{{{packageName}}}" */)),
  {{/each}}
  {{/if}}
};

export type ModalsKeysTypes = KeysOfRecord<typeof MODALS_CONFIG>;

export default MODALS_CONFIG;
