  "scripts": {
    "clean-base": "rm -rf ./node_modules",
    "clean": "rm -rf ./packages/*/node_modules",
    "clean-locks": "rm -rf ./packages/*/yarn.lock",
    "bootstrap": "lerna bootstrap",
    "create-env-vars": "plop --plopfile ./packages/generators/createEnvFile/developmentGenerator.js",
    "create-env-vars:prod": "plop --plopfile ./packages/generators/createEnvFile/productionGenerator.js",
    "//prestart": "yarn create-env-vars",
    "start": "yarn workspace app start",
    "//prebuild": "yarn create-env-vars:prod",
    "build": "yarn workspace app build",
    "reset": "yarn clean-base && yarn clean && yarn clean-locks && yarn bootstrap",
    "generate": "plop --plopfile ./packages/generators/index.js",
    "validate-exsys-deps": "node ./packages/validate-yarn-workspaces-deps",
    "prettify": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
